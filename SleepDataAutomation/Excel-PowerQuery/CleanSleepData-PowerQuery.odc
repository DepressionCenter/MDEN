  <odc:PowerQueryConnection odc:Type="OLEDB">
   <odc:ConnectionString>Provider=Microsoft.Mashup.OleDb.1;Data Source=$Workbook$;Location=SleepHRVEmailMarkers;Extended Properties=&quot;&quot;</odc:ConnectionString>
